const DEBUGlog = require('logToConsole');
DEBUGlog('[IGLOO][Snowplow :: Begin');

const injectScript = require('injectScript');
const fromWindow = require('copyFromWindow');
const setWindow = require('setInWindow');
const callWindow = require('callInWindow');
const queue = require('createQueue');
const argQueue = require('createArgumentsQueue');
const makeInteger = require('makeInteger');
const getCookie = require('getCookieValues');

const GSN = queue("GlobalSnowplowNamespace");

let tracker_name = 'snowplow';
let url = data.snowplow_js_url;
let sp_collector_mode = data.collector_mode="prod" ? data.prod_collector:data.dev_collector;

const get_payload = () => {
  let payload = {};

/********
*  Configuration Parameters
*********/
  if (data.appId) payload.appId = data.appId;
  if (data.platform) payload.platform = data.platform;
  if (data.cookieDomain) payload.cookieDomain = data.cookieDomain;
  if (data.discoverRootDomain) payload.discoverRootDomain = data.discoverRootDomain;
  if (data.cookieName) payload.cookieName = data.cookieName;
  if (data.encodeBase64) payload.encodeBase64 = data.encodeBase64;
  if (data.respectDoNotTrack) payload.respectDoNotTrack = data.respectDoNotTrack;
  if (data.userFingerprint) payload.userFingerprint = data.userFingerprint;
  if (data.userFingerprintSeed) payload.userFingerprintSeed = data.userFingerprintSeed;
  if (data.pageUnloadTimer) payload.pageUnloadTimer = makeInteger(data.pageUnloadTimer);
  if (data.forceSecureTracker) payload.forceSecureTracker = data.forceSecureTracker;
  
  if (data.eventMethod) payload.eventMethod = data.eventMethod;

  if (data.bufferSize) payload.bufferSize = makeInteger(data.bufferSize);
  if (data.maxPostBytes) payload.maxPostBytes = makeInteger(data.maxPostBytes); 
  if (data.cookieLifetime) payload.cookieLifetime = makeInteger(data.cookieLifetime);
  if (data.stateStorageStrategy) payload.stateStorageStrategy = data.stateStorageStrategy;

/********
*  Native Contexts Parameters
*********/
  payload.contexts = {};
  if (data.webPage) payload.contexts.webPage = data.webPage;
  if (data.performanceTiming) payload.contexts.performanceTiming = data.performanceTiming;
  if (data.gaCookies) payload.contexts.gaCookies = data.gaCookies;
  if (data.geolocation) payload.contexts.geolocation = data.geolocation;
  
  return payload;
};//END get_payload

if (!GSN.snowplow) { GSN(tracker_name); }
const call_snowplow = fromWindow(tracker_name) || argQueue(tracker_name,tracker_name + '.q');

/********
*  Init Tracker
*********/
DEBUGlog('[IGLOO][Snowplow :: Init');
call_snowplow('newTracker',data.namespace,sp_collector_mode,get_payload());

/********
*  Call Other Parameters
*********/

/* Activity Tracking */

if (data.enableActivityTracking) call_snowplow('enableActivityTracking', makeInteger(data.minimumVisitLength), makeInteger(data.heartbeat));

/* User Identification */ 

if (data.setUserId) call_snowplow('setUserId',data.setUserId);
if (data.setUserIdFromLocation) call_snowplow('setUserIdFromLocation',data.setUserIdFromLocation);
if (data.setUserIdFromReferrer) call_snowplow('setUserIdFromReferrer',data.setUserIdFromReferrer);

/* Others */ 

if (data.setCustomUrl) call_snowplow('setCustomUrl',data.setCustomUrl);
if (data.setReferrerUrl) call_snowplow('setReferrerUrl',data.setReferrerUrl);

/********
*  Fire Pageview
*********/
DEBUGlog('[IGLOO][Snowplow :: Pageview');
call_snowplow('trackPageView');

/********
*  Load Script
*********/
injectScript(url,data.gtmOnSuccess,data.gtmOnFailure,url);


